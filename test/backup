CREATE TABLE test (
    id integer NOT NULL,
    text text
);


ALTER TABLE public.test OWNER TO chris;

--
-- Name: test_id_seq; Type: SEQUENCE; Schema: public; Owner: chris
--

CREATE SEQUENCE test_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.test_id_seq OWNER TO chris;

--
-- Name: test_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: chris
--

ALTER SEQUENCE test_id_seq OWNED BY test.id;


--
-- Name: zettel_id_seq; Type: SEQUENCE; Schema: public; Owner: chris
--

CREATE SEQUENCE zettel_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.zettel_id_seq OWNER TO chris;

--
-- Name: zettel; Type: TABLE; Schema: public; Owner: chris; Tablespace: 
--

CREATE TABLE zettel (
    zettel_id integer DEFAULT nextval('zettel_id_seq'::regclass) NOT NULL,
    modul character varying(15) NOT NULL,
    zettel_nr smallint NOT NULL,
    abzugeben_bis date,
    abgegeben_am date,
    max_pkt smallint,
    erreichte_pkt double precision
);


ALTER TABLE public.zettel OWNER TO chris;

--
-- Name: id; Type: DEFAULT; Schema: public; Owner: chris
--

ALTER TABLE ONLY test ALTER COLUMN id SET DEFAULT nextval('test_id_seq'::regclass);


